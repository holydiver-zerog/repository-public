   "source": [
    "import cv2\n",
    "\n",
    "# カメラの初期化\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "# 静止画の取得\n",
    "ret, frame = cap.read()\n",
    "\n",
    "# 画像を保存\n",
    "if ret:\n",
    "    cv2.imwrite('img.jpg', frame)\n",
    "\n",
    "# 解放処理\n",
    "cap.release()"
   ]
